{
  "name": "enquiry-api",
  "version": "1.0.0",
  "description": "sending email",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon index.js",
    "worker": "nodemon worker.js",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:generate:?": "sequelize migration:generate --name",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "seed:generate:?": "sequelize seed:generate --name",
    "seed:undo:?": "sequelize db:seed:undo --seed"
  },
  "author": "Issaka Faisal",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bull": "^3.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^6.5.0",
    "throng": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
